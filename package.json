      "./src/index.ts"
    ]
  },
  "openclaw": {
    "extensions": [
      "./src/index.ts"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
