{
  "name": "@browserbasehq/openclaw-browserbase",
  "version": "0.1.0",
  "description": "OpenClaw plugin for Browserbase setup and skill installation",
  "packageManager": "pnpm@10.21.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "clawdbot",
    "openclaw",
    "plugin",
    "browserbase",
    "browser",
    "automation"
